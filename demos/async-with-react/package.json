{
  "name": "async-with-react",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Async with React",
    "manifest": "src/manifest.json",
    "main": "async-with-react.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "style-loader": "^1.1.3"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@skpm/fs": "^0.2.5",
    "@types/react": "^16.9.23",
    "mocha-js-delegate": "^0.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "sketch-module-web-view": "^3.1.4"
  },
  "author": "jiazhewang <neoddish@outlook.com>"
}
